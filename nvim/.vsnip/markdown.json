{
    "lookatme frontmatter": {
        "prefix": "fmlam",
        "description": "Creates frontmatter for lam slides",
        "body": [
            "---",
            "title: ${1}",
            "date: ${2:${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}}",
            "author: Robby Kiskanyan",
            "styles:",
            "  style: monokai",
            "  table:",
            "    column_spacing: 15",
            "  margin:",
            "    top: 3",
            "    bottom: 0",
            "  padding:",
            "    top: 3",
            "    bottom: 3",
            "---",
            "$0"
        ]
    },
    "jsx frontmatter": {
        "prefix": "jsxfm",
        "description": "Creates frontmatter for jsx markdown posts",
        "body": [
            "---",
            "title: ${1}",
            "description: ${2}",
            "date: \"${3:${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}}\"",
            "image: \"${4}\"",
            "author: \"Robby Kiskanyan\"",
            "tags: [\"${5}\"]",
            "---",
            "$0"
        ]
    }
}
